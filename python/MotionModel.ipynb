    "def sample_normal_distribution(variance):\n",
    "    rand_sum = 0\n",
    "    for i in range(12):\n",
    "        rand_sum = rand_sum + random.uniform(-1, 1)\n",
    "    return rand_sum * variance / 6\n"
   ]
  },
  {
